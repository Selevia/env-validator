{
  "name": "selevia/env-validator",
  "description": "The script for validate env variables",
  "type": "library",
  "license": "Copyright Selevia",
  "require": {
    "php": "^7.3 | ^8.0.0",
    "symfony/console": "^5.1",
    "vlucas/phpdotenv": "^5.0",
    "doctrine/collections": "^1.6",
    "beberlei/assert": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "codedungeon/phpunit-result-printer": "^0.30.1"
  },
  "autoload": {
    "psr-4": {
